{
  "name": "heavy-navbar-react",
  "sideEffects": false,
  "version": "0.0.1-0",
  "private": true,
  "description": "React specific wrapper for heavy-navbar",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
